{
	"manifest_version": 3,
	"name": "Yet Another Tracker You'll Abandon",
	"short_name": "YAT",
	"author": "Jun Ivanne Dalman",
	"version": "1.0.0",
	"version_name": "1.0 beta",
	"description": "Bloat",
	"homepage_url": "https://github.com/yvantot",
	"permissions": [
		"storage",
		"unlimitedStorage"
	],
	"chrome_url_overrides": {
		"newtab": "./html/newtab.html?mode=newtab"
	},
	"action": {
		"default_popup": "./html/newtab.html?mode=popup",
		"default_title": "Bloat"
	},
	"background": {
		"service_worker": "./js/background.js"
	}
}